@namespace("sysflow.flow")
protocol SysFlow {
import idl "../type/datatypes.avdl";
record FileFlow {
  sysflow.type.OID procOID; // host ID + monotonic process creation time + host PID
  timestamp_ms ts;
  long tid;
  int opFlags;
  int openFlags;
  timestamp_ms endTs;
  sysflow.type.FOID fileOID;
  int fd;
  long numRRecvOps;
  long numWSendOps;
  long numRRecvBytes;
  long numWSendBytes;
}
}
